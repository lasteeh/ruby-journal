<p>test login page</p>

<% if flash[:notice] %>
    <p><%= flash[:notice] %></p>
<% end %>

<%= form_with scope: :user, url: login_path, local: true do |form| %>

    <%= form.label :email %>
    <%= form.email_field :email %>
    
    <%= form.label :password %>
    <%= form.password_field :password %>


    <%= form.submit 'login' %>
<% end %>